<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

    <title>Hacktiv8 Student Grading</title>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" href="/admins">Home</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" href="/admins/list-teacher">Teachers</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="/admins/list-student">Students</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admins/list-lecture">Lectures</a>
          </li>
        </ul>
      </div>

      <div class="my-sm-0">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" href="/logout">Logout</a>
          </li>
        </ul>
      </div>
    </nav>


<div class="jumbotron container">
  <h1 class="display-4">List Student Hacktiv8</h1>
  <p class="lead">This is simple Student Grading website.</p>
  <hr class="my-4">
  <table border="1" width="100%">
    <thead>
      <th>No</th>
      <th>Name</th>
      <th>Gender</th>
      <th>Birth Date</th>
      <th>Phone Number</th>
      <th>Email</th>
      <th>Username</th>
      <th>Discord Id</th>
      <th>Actions</th>
    </thead>
    <tbody>
      <% data.forEach((el, i) => { %>
        <% i += 1 %>
        <tr>
          <td><%= i %></td>
          <td><%= el.name %></td>
          <td><%= el.gender %></td>
          <td><%= el.birth_date %></td>
          <td><%= el.phone_number %></td>
          <td><%= el.email %></td>
          <td><%= el.username %></td>
          <td><%= el.discordId %></td>
          <td><a class="btn btn-primary" href="/admins/list-student/<%= el.id %>/delete">Delete</a> <a class="btn btn-primary" href="/admins/list-student/<%= el.id %>/edit">Edit</a> <a class="btn btn-primary" href="/admins/list-student/<%= el.id %>/edit-lecture">Edit Lectures</a></td>


        </tr>
      <% }); %>
    </tbody>
  </table>
  <br>
  <a class="btn btn-primary btn-lg" href="/admins/add-student" role="button">Add Student</a>
</div>

  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
  </body>
</html>